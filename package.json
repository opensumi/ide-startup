{
  "name": "startup",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/opensumi/ide-startup.git",
    "type": "https"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run prepare",
    "download:extensions": "node scripts/download.js",
    "compile:browser": "webpack --config ./configs/webpack.browser.config.js --progress --color",
    "compile:node": "webpack --config ./configs/webpack.node.config.js --progress --color",
    "compile:worker-host": "webpack --config ./configs/webpack.worker-host.config.js --progress --color",
    "compile:ext-host": "webpack --config ./configs/webpack.ext-host.config.js --progress --color",
    "prepare": "npm run compile:worker-host && npm run compile:ext-host && DEBUG=install-ext npm run download:extensions",
    "build": "npm run compile:browser && npm run compile:node && npm run compile:worker-host && npm run compile:ext-host",
    "start": "run-p start:client start:server start:webview",
    "start:client": "NODE_ENV=development webpack-dev-server --config ./configs/webpack.browser.config.js --progress --color --watch",
    "start:webview": "webpack-dev-server --config ./configs/webpack.webview.config.js",
    "start:server": "NODE_ENV=development EXT_MODE=js KTLOG_SHOW_DEBUG=1 node --inspect=9991 -r ts-node/register -r tsconfig-paths/register ./src/node/index.ts"
  },
